{
  "name": "webpack-practice",
  "version": "1.0.0",
  "description": "Proyecto del curso de Webpack en Platzi",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:manual": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js"
  },
  "keywords": [
    "webpack",
    "baby",
    "steps"
  ],
  "author": "Mayra Cruz",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "0.8.0",
    "style-loader": "1.0.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.9.0"
  },
  "dependencies": {
    "@babel/runtime": "7.6.3",
    "react": "16.11.0",
    "react-dom": "16.11.0"
  }
}
